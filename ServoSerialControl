
#include<Servo.h>
Servo myservo;
int pos = 0;


void setup(){
Serial.begin(9600);
myservo.attach(9);
}

void loop(){
if (Serial.available())
{
int pos = Serial.parsInt();
if(pos < 10)
{
Serial.print(">");
Serial.println(pos);
Serial.println("Cannot execute command, value too low");
}
if (pos >= 10 && pos < 170)
{
Serial.print("> ")l
Serial.println(pos);
Serial.print("turning servo to ");
Serial.print(pos);
Serial.println(" degrees");
myservo.write(pos);
}


}

}
